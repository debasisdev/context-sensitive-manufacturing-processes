                <Condition definitionLanguage="http://www.w3.org/TR/xpath">
                    <DefinitionContent>
                        <Expression>//Context[@name='availableWorkers']/ContextDefinition/DefinitionContent[SenseValue&lt;10]/SenseValue/text() | //Context[@name='infraredSensorStatus']/ContextDefinition/DefinitionContent[contains(SenseValue,'Okay')]/SenseValue/text() | //Context[@name='shockDetectorStatus']/ContextDefinition/DefinitionContent[not(contains(SenseValue,'Okay'))]/SenseValue/text()</Expression>
                    </DefinitionContent>
                </Condition>
            </ContextExpression>
        </InitialContexts>
        <Weight>0.15</Weight>
    </ProcessDefinition>
</tProcessModel>