                <Condition definitionLanguage="http://www.w3.org/TR/xpath">
                    <DefinitionContent>
                        <Expression>//Context[@name='unitsOrdered']/ContextDefinition/DefinitionContent[SenseValue&gt;1000]/SenseValue/text() | //Context[@name='infraredSensorStatus']/ContextDefinition/DefinitionContent[contains(SenseValue,'Okay')]/SenseValue/text() | //Context[@name='shockDetectorStatus']/ContextDefinition/DefinitionContent[not(contains(SenseValue,'Okay'))]/SenseValue/text()</Expression>
                    </DefinitionContent>
                </Condition>
            </ContextExpression>
			<ContextExpression id="CON3D" name="LowHR-InfraredSensorFailed" targetNamespace="http://www.uni-stuttgart.de/iaas/cmp/v1/packaging">
                <Documentation>This context is required to decide this process is okay for the purpose or not.</Documentation>
                <Condition definitionLanguage="http://www.w3.org/TR/xpath">
