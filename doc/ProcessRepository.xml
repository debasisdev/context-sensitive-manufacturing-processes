                    <DefinitionContent>
                        <Expression>//Context[@name='availableWorkers']/ContextDefinition/DefinitionContent[SenseValue&lt;10]/SenseValue/text() | //Context[@name='infraredSensorStatus']/ContextDefinition/DefinitionContent[not(contains(SenseValue,'Okay'))]/SenseValue/text() | //Context[@name='shockDetectorStatus']/ContextDefinition/DefinitionContent[not(contains(SenseValue,'Okay'))]/SenseValue/text()</Expression>
                    </DefinitionContent>
                </Condition>
            </ContextExpression>
			<ContextExpression id="CON3F" name="LowHR-ShockDetectorFailed" targetNamespace="http://www.uni-stuttgart.de/iaas/cmp/v1/packaging">
