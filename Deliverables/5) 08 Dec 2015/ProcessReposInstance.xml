<?xml version="1.0" encoding="UTF-8"?>
<ProcessData primeintention="SealAndSortPackets">
	<ProcessDefinition name="ManualWorkflow" targetNamespace="http://www.uni-stuttgart.de/iaas/cmp/v1/packaging">
		<Intention intvalidator="/res/IntentionValidator.xslt">HighThroughput</Intention>
		<Context convalidator="/res/ContextValidator.xslt">
			<Required>availableWorkers</Required>
			<Required>unitsOrdered</Required>
			<Required>infraredSensorStatus</Required>
			<Required>shockDetectorStatus</Required>
		</Context>
		<Process type="BPEL" engine="Activiti">
			<Identifier></Identifier> <!--Here the BPEL or BPMN Model will be stored or a link to related Model will be given as URI-->
		</Process>
	</ProcessDefinition>
	
	<ProcessDefinition name="SemiAutomatedWorkflowWithoutRepairing" targetNamespace="http://www.uni-stuttgart.de/iaas/cmp/v1/packaging">
		<Intention intvalidator="/res/IntentionValidator.xslt">HighThroughput</Intention>
		<Context convalidator="/res/ContextValidator.xslt">
			<Required>availableWorkers</Required>
			<Required>unitsOrdered</Required>
			<Required>infraredSensorStatus</Required>
			<Required>shockDetectorStatus</Required>
		</Context>
		<Process type="BPMN" engine="Stardust">
			<Identifier></Identifier>
		</Process>
	</ProcessDefinition>
	
	<ProcessDefinition name="SemiAutomatedWorkflowWithRepairing" targetNamespace="http://www.uni-stuttgart.de/iaas/cmp/v1/packaging">
		<Intention intvalidator="/res/IntentionValidator.xslt">HighThroughput</Intention>
		<Context convalidator="/res/ContextValidator.xslt">
			<Required>availableWorkers</Required>
			<Required>unitsOrdered</Required>
			<Required>infraredSensorStatus</Required>
			<Required>shockDetectorStatus</Required>
		</Context>
		<Process type="BPMN" engine="StarDust">
			<Identifier></Identifier>
		</Process>
	</ProcessDefinition>
</ProcessData>