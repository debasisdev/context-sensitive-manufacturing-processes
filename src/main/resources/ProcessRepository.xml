				<ipsm:Intention name="highThroughput" targetNamespace="http://www.uni-stuttgart.de/iaas/cmp/v1/packaging">
                    <ipsm:Documentation>Takt-time should be low.</ipsm:Documentation>
                </ipsm:Intention>
				<ipsm:Intention name="lowMaintenance" targetNamespace="http://www.uni-stuttgart.de/iaas/cmp/v1/packaging">
                    <ipsm:Documentation>Company requires new machinery and repeated maintenance for less on-demand repair activity.</ipsm:Documentation>
                </ipsm:Intention>
            </ipsm:SubIntentions>
        </ipsm:TargetIntention>
        <ipsm:ContextRules>
            <ipsm:ContextExpression id="CON3A" name="HighDemand-InfraredSensorFailed" targetNamespace="http://www.uni-stuttgart.de/iaas/cmp/v1/packaging">
                <ipsm:Documentation>This context is required to decide this process is okay for the purpose or not.</ipsm:Documentation>
                <ipsm:Condition definitionLanguage="http://www.w3.org/TR/xpath">
                    <ipsm:DefinitionContent>
                        <ipsm:tManufacturingContent>
                            <cmp:Expression>//Context[@name='unitsOrdered']/ContextDefinition/DefinitionContent/tManufacturingContent[SenseValue&gt;1000]/SenseValue/text() | //Context[@name='infraredSensorStatus']/ContextDefinition/DefinitionContent/tManufacturingContent[not(contains(SenseValue,'Okay'))]/SenseValue/text() | //Context[@name='shockDetectorStatus']/ContextDefinition/DefinitionContent/tManufacturingContent[contains(SenseValue,'Okay')]/SenseValue/text()</cmp:Expression>
                        </ipsm:tManufacturingContent>
                    </ipsm:DefinitionContent>
                </ipsm:Condition>
            </ipsm:ContextExpression>
