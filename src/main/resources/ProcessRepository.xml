<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ipsm:tProcessDefinitions xmlns:ipsm="http://www.uni-stuttgart.de/iaas/ipsm/v0.2/" xmlns:pref="http://docs.oasis-open.org/tosca/ns/2011/12" xmlns:cmp="http://www.uni-stuttgart.de/iaas/cmp/v0.1/">
    <cmp:ProcessDefinition processType="http://www.omg.org/spec/BPMN/2.0/" id="PMX001" name="ManualPacking" targetNamespace="http://www.uni-stuttgart.de/iaas/cmp/v1/packaging">
        <ipsm:Documentation>This is a dummy Manual Model.</ipsm:Documentation>
        <ipsm:ProcessContent>
            <ipsm:tManufacturingContent>
                <cmp:Resource>res/dev/man.bpmn</cmp:Resource>
                <cmp:Weight>0.30</cmp:Weight>
            </ipsm:tManufacturingContent>
        </ipsm:ProcessContent>
        <ipsm:TargetIntention name="SealAndSortPackets" targetNamespace="http://www.uni-stuttgart.de/iaas/cmp/v1/packaging">
            <ipsm:Documentation>The primary goal/intention is to seal and sort the packets as per the business rule.</ipsm:Documentation>
            <ipsm:SubIntentions>
                <ipsm:Intention name="highHRUtilization" targetNamespace="http://www.uni-stuttgart.de/iaas/cmp/v1/packaging">
                    <ipsm:Documentation>Work must be done in such a way that no employees sit idle in the premises of company.</ipsm:Documentation>
                </ipsm:Intention>
            </ipsm:SubIntentions>
        </ipsm:TargetIntention>
        <ipsm:ContextRules>
            <ipsm:ContextExpression id="CON1" name="HighHR-LowDemand" targetNamespace="http://www.uni-stuttgart.de/iaas/cmp/v1/packaging">
                <ipsm:Documentation>It depicts a business situation where demand is pretty low and available workers are more.</ipsm:Documentation>
                <ipsm:Condition definitionLanguage="http://www.w3.org/TR/xpath">
                    <ipsm:DefinitionContent>
                        <ipsm:tManufacturingContent>
                            <cmp:Expression>//Context[@name='availableWorkers']/ContextDefinition/DefinitionContent/tManufacturingContent[SenseValue&gt;=10]/SenseValue/text() | //Context[@name='unitsOrdered']/ContextDefinition/DefinitionContent/tManufacturingContent[SenseValue&lt;=1000]/SenseValue/text()</cmp:Expression>
                        </ipsm:tManufacturingContent>
                    </ipsm:DefinitionContent>
                </ipsm:Condition>
            </ipsm:ContextExpression>
        </ipsm:ContextRules>
    </cmp:ProcessDefinition>
	<cmp:ProcessDefinition processType="http://www.omg.org/spec/BPMN/2.0/" id="PSM001" name="SemiAutomatedPacking" targetNamespace="http://www.uni-stuttgart.de/iaas/cmp/v1/packaging">
        <ipsm:Documentation>This is a dummy semi automated model without repair activity.</ipsm:Documentation>
        <ipsm:ProcessContent>
            <ipsm:tManufacturingContent>
                <cmp:Resource>res/dev/semi.bpmn</cmp:Resource>
                <cmp:Weight>0.55</cmp:Weight>
            </ipsm:tManufacturingContent>
        </ipsm:ProcessContent>
        <ipsm:TargetIntention name="SealAndSortPackets" targetNamespace="http://www.uni-stuttgart.de/iaas/cmp/v1/packaging">
            <ipsm:Documentation>The primary goal/intention is to seal and sort the packets as per the business rule.</ipsm:Documentation>
            <ipsm:SubIntentions>
                <ipsm:Intention name="highAutomation" targetNamespace="http://www.uni-stuttgart.de/iaas/cmp/v1/packaging">
                    <ipsm:Documentation>Work must be automated more to gain higher productivity.</ipsm:Documentation>
                </ipsm:Intention>
				<ipsm:Intention name="highThroughput" targetNamespace="http://www.uni-stuttgart.de/iaas/cmp/v1/packaging">
                    <ipsm:Documentation>Takt-time should be low.</ipsm:Documentation>
                </ipsm:Intention>
				<ipsm:Intention name="highHRUtilization" targetNamespace="http://www.uni-stuttgart.de/iaas/cmp/v1/packaging">
                    <ipsm:Documentation>Work must be done in such a way that no employees sit idle in the premises of company.</ipsm:Documentation>
                </ipsm:Intention>
            </ipsm:SubIntentions>
        </ipsm:TargetIntention>
        <ipsm:ContextRules>
            <ipsm:ContextExpression id="CON2A" name="HighDemand-SensorsOkay" targetNamespace="http://www.uni-stuttgart.de/iaas/cmp/v1/packaging">
                <ipsm:Documentation>It depicts a business situation where demand is pretty high and all machines are working perfectly.</ipsm:Documentation>
                <ipsm:Condition definitionLanguage="http://www.w3.org/TR/xpath">
                    <ipsm:DefinitionContent>
                        <ipsm:tManufacturingContent>
                            <cmp:Expression>//Context[@name='unitsOrdered']/ContextDefinition/DefinitionContent/tManufacturingContent[SenseValue&gt;1000]/SenseValue/text() | //Context[@name='infraredSensorStatus']/ContextDefinition/DefinitionContent/tManufacturingContent[contains(SenseValue,'Okay')]/SenseValue/text() | //Context[@name='shockDetectorStatus']/ContextDefinition/DefinitionContent/tManufacturingContent[contains(SenseValue,'Okay')]/SenseValue/text()</cmp:Expression>
                        </ipsm:tManufacturingContent>
                    </ipsm:DefinitionContent>
                </ipsm:Condition>
            </ipsm:ContextExpression>
			<ipsm:ContextExpression id="CON2B" name="LowHR-SensorsOkay" targetNamespace="http://www.uni-stuttgart.de/iaas/cmp/v1/packaging">
                <ipsm:Documentation>It depicts a business situation where there is very less workers available and all machines are working perfectly.</ipsm:Documentation>
                <ipsm:Condition definitionLanguage="http://www.w3.org/TR/xpath">
                    <ipsm:DefinitionContent>
                        <ipsm:tManufacturingContent>
                            <cmp:Expression>//Context[@name='availableWorkers']/ContextDefinition/DefinitionContent/tManufacturingContent[SenseValue&lt;10]/SenseValue/text() | //Context[@name='infraredSensorStatus']/ContextDefinition/DefinitionContent/tManufacturingContent[contains(SenseValue,'Okay')]/SenseValue/text() | //Context[@name='shockDetectorStatus']/ContextDefinition/DefinitionContent/tManufacturingContent[contains(SenseValue,'Okay')]/SenseValue/text()</cmp:Expression>
                        </ipsm:tManufacturingContent>
                    </ipsm:DefinitionContent>
                </ipsm:Condition>
            </ipsm:ContextExpression>
        </ipsm:ContextRules>
    </cmp:ProcessDefinition>
	<cmp:ProcessDefinition processType="http://www.omg.org/spec/BPMN/2.0/" id="PSM002" name="SemiAutomatedRepairPacking" targetNamespace="http://www.uni-stuttgart.de/iaas/cmp/v1/packaging">
        <ipsm:Documentation>This is a dummy semi automated model with repair activity.</ipsm:Documentation>
        <ipsm:ProcessContent>
            <ipsm:tManufacturingContent>
                <cmp:Resource>res/dev/semirepair.bpmn</cmp:Resource>
                <cmp:Weight>0.15</cmp:Weight>
            </ipsm:tManufacturingContent>
        </ipsm:ProcessContent>
        <ipsm:TargetIntention name="SealAndSortPackets" targetNamespace="http://www.uni-stuttgart.de/iaas/cmp/v1/packaging">
            <ipsm:Documentation>The primary goal/intention is to seal and sort the packets as per the business rule.</ipsm:Documentation>
            <ipsm:SubIntentions>
