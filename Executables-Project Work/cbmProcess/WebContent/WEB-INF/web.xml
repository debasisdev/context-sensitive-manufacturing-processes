  	<filter-name>IppPortalLoginFilter</filter-name>
  	<url-pattern>*.iface</url-pattern>
  	<dispatcher>REQUEST</dispatcher>
  	<dispatcher>FORWARD</dispatcher>
  </filter-mapping>
  <filter-mapping>
  	<filter-name>IppPortalCharsetFilter</filter-name>
  	<url-pattern>/uploadHtml</url-pattern>
  </filter-mapping>
  <filter-mapping>
  	<filter-name>touchSessionFilter</filter-name>
  	<url-pattern>/services/rest/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
  	<filter-name>Shell Index Redirecting Filter</filter-name>
  	<url-pattern>/main.html</url-pattern>
  	<dispatcher>REQUEST</dispatcher>
  	<dispatcher>FORWARD</dispatcher>
  </filter-mapping>
