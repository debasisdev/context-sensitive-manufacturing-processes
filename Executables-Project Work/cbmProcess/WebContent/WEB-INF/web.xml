  	<param-name>facelets.RESOURCE_RESOLVER</param-name>
  	<param-value>org.eclipse.stardust.ui.web.viewscommon.common.PortalPluginFaceletsResourceResolver</param-value>
  </context-param>
  <context-param>
  	<param-name>javax.faces.DEFAULT_SUFFIX</param-name>
