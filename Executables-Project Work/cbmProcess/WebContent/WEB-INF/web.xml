  		<param-name>skipExtenssions</param-name>
  		<param-value>.xhtml,.jsp,.class,.xml,.properties</param-value>
  	</init-param>
  </filter>
  <filter-mapping>
  	<filter-name>portalPluginContentFilter</filter-name>
  	<url-pattern>/plugins/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
  	<filter-name>portalPluginContentFilter</filter-name>
  	<url-pattern>/skins/*</url-pattern>
  </filter-mapping>
